{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "YOUR_ACCOUNT_NUMBER.dkr.ecr.us-east-1.amazonaws.com/ai-academy-backend:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "5000"
    }
  ],
  "Environment": [
    {
      "Name": "FLASK_ENV",
      "Value": "production"
    },
    {
      "Name": "DB_HOST",
      "Value": "your-rds-endpoint.amazonaws.com"
    },
    {
      "Name": "DB_USER",
      "Value": "admin"
    },
    {
      "Name": "DB_PASSWORD",
      "Value": "your-db-password"
    },
    {
      "Name": "DB_NAME",
      "Value": "ai_academy"
    }
  ]
}